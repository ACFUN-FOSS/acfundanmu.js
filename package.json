{
  "name": "acfunlive-http-api",
  "version": "1.0.0",
  "description": "AcFun直播HTTP API库 - 基于Go版本移植的JavaScript实现",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "prepublishOnly": "npm run build",
    "proto:compile": "pbjs -t static-module -w commonjs -o src/proto/acfun.js protos/**/*.proto && pbts -o src/proto/acfun.d.ts src/proto/acfun.js"
  },
  "keywords": [
    "acfun",
    "live",
    "danmu",
    "api",
    "http"
  ],
  "author": "AcFunLive Team",
  "license": "MIT",
  "dependencies": {
    "@types/crypto-js": "^4.2.2",
    "@types/protobufjs": "^6.0.0",
    "@types/ws": "^8.18.1",
    "axios": "^1.6.7",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.4.1",
    "eventsource": "^4.0.0",
    "express": "^4.18.0",
    "fast-json-stringify": "^5.16.1",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0",
    "natural-compare": "^1.4.0",
    "protobufjs": "^7.5.4",
    "qrcode": "^1.5.4",
    "qrcode-reader": "^1.0.4",
    "tc-qrcode": "^1.1.0",
    "uuid": "^9.0.0",
    "winston": "^3.11.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.0",
    "@types/express": "^4.17.0",
    "@types/google-protobuf": "^3.15.12",
    "@types/jest": "^29.5.14",
    "@types/morgan": "^1.9.0",
    "@types/natural-compare": "^1.4.3",
    "@types/node": "^20.19.21",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "nodemon": "^3.0.3",
    "protobufjs-cli": "^1.1.3",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
